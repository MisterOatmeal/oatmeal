Object = {
    require = "external/classic",
    class = "Object",
    parameters =
    {
        "extend function",
        "implement function",
        "is function",
        "__tostring function",
        "__call function",
        "super object",
        "new fun(object, ...)"
    },
}

local require_list = {
    -- Mixins
    'Ferret/Mixins/Translation',
    -- Data enums and objects
    'Ferret/Data/Translatable', 'Ferret/Data/Conditions', 'Ferret/Data/Events',
    'Ferret/Data/Requests', 'Ferret/Data/Jobs', 'Ferret/Data/Name',
    'Ferret/Data/Objects', 'Ferret/Data/Status', 'Ferret/Data/Weather',
    'Ferret/Data/Version', 'Ferret/Data/Node',
    -- Other classes/utilities
    'Ferret/Targetable', 'Ferret/Timer', 'Ferret/Sandtimer', 'Ferret/Pathfinding',
    -- Base extension
    'Ferret/Extensions/Extension',
    -- Addons
    'Ferret/Addons/Addons',
    -- Actions
    'Ferret/Actions/Actions',
}

for _, path in ipairs(require_list) do
    require(path)
end

